<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_medium"
        android:layout_marginTop="@dimen/margin_large"
        android:gravity="left"
        android:text="Latest Feedback"
        android:textColor="@color/black_real"
        android:textSize="@dimen/text_large"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_marginLeft="@dimen/margin_medium"
        android:layout_marginTop="@dimen/margin_small"
        android:gravity="left"
        android:text="Test on live"
        android:textColor="@color/black_real"
        android:textSize="@dimen/text_small"
        android:textStyle="normal" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginLeft="@dimen/margin_medium"
        android:layout_marginTop="@dimen/margin_small"
        android:gravity="left"
        android:text="Bhaskar Dhote"
        android:textColor="@color/black_real"
        android:textSize="@dimen/text_small"
        android:textStyle="normal" />


    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_marginLeft="@dimen/margin_medium"
        android:layout_marginTop="@dimen/margin_small"
        android:gravity="left"
        android:text="Parision Macao"
        android:textColor="@color/black_real"
        android:textSize="@dimen/text_small"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_marginLeft="@dimen/margin_medium"
        android:layout_marginTop="@dimen/margin_small"
        android:gravity="left"
        android:text="February 20 2018"
        android:textColor="@color/black_real"
        android:textSize="@dimen/text_small"
        android:textStyle="normal" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_marginLeft="@dimen/margin_medium"
        android:layout_marginRight="@dimen/margin_medium"
        android:layout_marginTop="@dimen/margin_large">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/moveUP"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginRight="@dimen/margin_medium"
                android:src="@mipmap/arrow_up"
                android:visibility="visible" />

            <LinearLayout
                android:id="@+id/middle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/moveUP"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/middle_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0 of 7"
                    android:textColor="@color/black_real"
                    android:textSize="@dimen/text_small"
                    android:textStyle="normal" />
            </LinearLayout>


            <ImageView
                android:id="@+id/moveDown"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginLeft="@dimen/margin_medium"
                android:layout_toRightOf="@+id/middle"
                android:src="@mipmap/arrow_down"
                android:visibility="visible" />
        </RelativeLayout>


    </RelativeLayout>

</RelativeLayout>